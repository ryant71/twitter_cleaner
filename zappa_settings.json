{
    "production": {
        "events": [{
            "function": "twitter_clean.lambda_handler",
            // "expressions": ["cron(15 0 * * ? *)"]
            "expression": "rate(6 hours)"
        }],
        "aws_region": "eu-west-1",
        "profile_name": "zappa",
        "project_name": "twitter-cleaner",
        "runtime": "python3.7",
        "s3_bucket": "zappa-twit",
        "keep_warm": false,
        "apigateway_enabled": false,
        "memory_size": 128,
        "debug": false,
        "log_level": "CRITICAL",
        "timeout_seconds": 300
    }
}
